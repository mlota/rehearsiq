{
	"version": 2,
	"projectSettings": {
		"framework": null,
		"buildCommand": "nx run-many --target=build --projects=rehearsiq,rehearsiq-api"
	},
	"builds": [
		{
			"src": "dist/apps/rehearsiq-api/apps/rehearsiq-api/api/**/*.ts",
			"use": "@vercel/node"
		},
		{
			"src": "dist/apps/rehearsiq/browser/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{ "src": "/api/(.*)", "dest": "dist/apps/rehearsiq-api/apps/rehearsiq-api/api/$1.ts" },
		{ "src": "/(.*)", "dest": "dist/apps/rehearsiq/browser/$1" }
	]
}
